{
	"name": "manga-ts-scrapper",
	"version": "0.0.1",
	"description": "A Scrapper build on NodeJs using Puppeteer.",
	"main": "server/server.ts",
	"repository": "https://github.com/johncovv/manga-ts-scrapper",
	"author": "John Covv <contato@johncovv.com>",
	"license": "MIT",
	"private": false,
	"scripts": {
		"dev": "ts-node-dev --transpile-only --inspect --ignore-watch node_modules ./server/server.ts",
		"build": "tsc",
